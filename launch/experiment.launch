<launch>
  <arg name="save_dir" default="$(find moveit_recorder)/videos" />
  <arg name="regex" default="view." />

  <node name="experiment_node" pkg="moveit_recorder" type="experiment_node" 
    args="--save_dir $(arg save_dir) --regex $(arg regex)" 
    respawn="false" output="screen">
  </node>
  
</launch>

